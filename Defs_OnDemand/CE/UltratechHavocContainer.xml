<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingCategoryDef>
      <defName>AmmoUltratechMatterContainer</defName>
      <label>ultratech matter container</label>
      <parent>AmmoAdvanced</parent>
      <iconPath>UI/Icons/ThingCategories/Ammo</iconPath>
